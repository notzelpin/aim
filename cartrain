local partToDelete = game:GetService("ReplicatedStorage").Remotes.Rejoin



partToDelete:Destroy() 
wait()
_G.E = true
while _G.E == true do
wait()
local args = {
    [1] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Merchant"):InvokeServer(unpack(args))
wait()
local args = {
    [1] = 2
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Merchant"):InvokeServer(unpack(args))
wait()

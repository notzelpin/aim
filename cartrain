    [1] = 3
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Merchant"):InvokeServer(unpack(args))
wait()
local args = {
    [1] = 4
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Merchant"):InvokeServer(unpack(args))
wait()
local args = {
    [1] = 5
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Merchant"):InvokeServer(unpack(args))
wait()
local args = {
    [1] = 6
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Merchant"):InvokeServer(unpack(args))
end
wait()
while wait() do
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Claim Reward"):InvokeServer()

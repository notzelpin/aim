local args = {
    [1] = "UpdateInventory",
    [2] = "Accessory",
    [3] = "Champion's Earrings"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Server"):FireServer(unpack(args))
wait()
